import pandas as pd
emp_details={'emp_id':pd.Series([1,2,3,4],index=['a','b','c','d']),
             'ename':pd.Series (['joey','nick','glen','Ben'],index=['a','b','c','d']),
             'position':pd.Series(['Devoloper','Cyber_security','Data_Engineer','Gamer'],index=['a','b','c','d']),
             'Rating':pd.Series([8,6,7,9],index=['a','b','c','d']),
             'Previous_Rating':pd.Series([7,5,6,8],index=['a','b','c','d'])}
print(emp_details)
emp_details_1=pd.DataFrame(emp_details)             
print(emp_details_1)
